generator client {
  provider   = "prisma-client-js"
  engineType = "client"
}

datasource db {
  provider = "postgresql"
  schemas  = ["auth", "public"]
}

model User {
  id                       String                @id @default(cuid()) @db.Uuid
  email                    String
  full_name                String?
  created_at               DateTime?             @default(now()) @db.Timestamptz(6)
  timezone                 String                @default("UTC")
  date_format              String                @default("DD/MM/YYYY")
  language_preference      String                @default("en")
  last_password_change     DateTime?             @default(now()) @db.Timestamptz(6)
  suspicious               Boolean               @default(false)
  suspicious_until         DateTime?             @db.Timestamptz(6)
  lemonsqueezy_customer_id String?               @unique @map("lemonsqueezy_customer_id")
  billing_tier             billing_tier          @default(free)
  auth_provider            String                @default("email")
  subscriptions            Subscription[]
  users                    users                 @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  kw_filter_presets        kw_filter_presets[]
  kw_serp_tasks            kw_serp_tasks[]
  notification_settings    NotificationSettings?
  userProjects             UserProject[]

  @@index([suspicious_until])
  @@map("core_profiles")
  @@schema("public")
}

model BillCredit {
  user_id       String   @id @db.Uuid
  credits_total Int      @default(0)
  credits_used  Int      @default(0)
  updated_at    DateTime @default(now()) @updatedAt @db.Timestamptz(6)
  user          users    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("bill_credits")
  @@schema("public")
}

model BillTransaction {
  id                    String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id               String   @db.Uuid
  amount                Int
  idempotency_key       String   @unique
  type                  String
  created_at            DateTime @default(now()) @db.Timestamptz(6)
  description           String?
  lemonsqueezy_order_id String?
  feature_name          String?
  metadata              Json     @default("{}")
  user                  users    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("bill_transactions")
  @@schema("public")
}

model Keyword {
  id               String      @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  slug             String      @unique
  keyword          String
  country_code     String
  match_type       String
  raw_data         Json?
  analysis_data    Json?
  last_labs_update DateTime    @default(now()) @db.Timestamptz(6)
  last_serp_update DateTime?   @db.Timestamptz(6)
  last_accessed_at DateTime    @default(now()) @db.Timestamptz(6)
  language_code    String      @default("en")
  location_code    Int?        @map("location_code")
  loss_percentage  Float?      @map("loss_percentage")
  rtv_score        Int?        @map("rtv_score")
  device_type      device_type @default(desktop)
  rankings         Ranking[]

  @@unique([keyword, location_code, language_code, device_type])
  @@index([keyword, country_code, device_type], map: "idx_kw_cache_device_lookup")
  @@index([last_accessed_at])
  @@map("kw_cache")
  @@schema("public")
}

model Subscription {
  id                 String     @id @default(cuid())
  user_id            String     @db.Uuid
  ls_subscription_id String     @unique @map("ls_subscription_id")
  variant_id         String
  status             sub_status
  renews_at          DateTime?  @db.Timestamptz(6)
  ends_at            DateTime?  @db.Timestamptz(6)
  created_at         DateTime   @default(now()) @db.Timestamptz(6)
  updated_at         DateTime   @default(now()) @updatedAt @db.Timestamptz(6)
  user               User       @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("billing_subscriptions")
  @@schema("public")
}

model Ranking {
  id         String      @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  project_id String      @db.Uuid
  keyword_id String      @db.Uuid
  position   Int
  url        String?
  device     device_type
  checked_at DateTime    @default(now()) @db.Timestamptz(6)
  keyword    Keyword     @relation(fields: [keyword_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([project_id])
  @@index([project_id, keyword_id, checked_at(sort: Desc)], map: "idx_rank_history_project_keyword_checked")
  @@map("rank_history")
  @@schema("public")
}

model ContentPerformance {
  id         String         @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  project_id String         @db.Uuid
  url        String
  status     content_status
  created_at DateTime       @default(now()) @db.Timestamptz(6)
  updated_at DateTime       @default(now()) @updatedAt @db.Timestamptz(6)

  @@index([project_id])
  @@map("content_performance")
  @@schema("public")
}

model TrendWatchlist {
  id             String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  project_id     String   @db.Uuid
  topic          String
  growth_percent Int
  created_at     DateTime @default(now()) @db.Timestamptz(6)
  updated_at     DateTime @default(now()) @updatedAt @db.Timestamptz(6)

  @@index([project_id])
  @@map("trend_watchlist")
  @@schema("public")
}

model UserProject {
  id            String        @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userid        String        @db.Uuid
  projectname   String        @db.VarChar(50)
  domain        String
  targetcountry String        @default("US") @map("target_country")
  icon          String?
  gscpropertyid String?       @map("gsc_property_id")
  createdat     DateTime      @default(now())
  updatedat     DateTime      @updatedAt
  user          User          @relation(fields: [userid], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([userid, domain])
  @@index([userid])
  @@map("userprojects")
  @@schema("public")
}

